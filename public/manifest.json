{
  "manifest_version": 2,

  "name" : "DSM Meal Extension",
  "description" : "Chrome meal extension program for DSM students",
  "version" : "2.0.0",
  "author" : "Ethan Cho",

  "browser_action" : {
    "default_popup" : "index.html"
  },

  "icons": {
    "16" : "./icons/icon16.png",
    "24" : "./icons/icon24.png",
    "32" : "./icons/icon32.png",
    "64" : "./icons/icon64.png",
    "128" :"./icons/icon128.png"
  },

  "permissions" : [
    "tabs",
    "<all_urls>",
    "storage"
  ]
}
